{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["./js/scripts.js", "./js/chess3d.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "docs"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "private": true,
        "readme": "README.md",
        "template": "./node_modules/minami"
    }
}